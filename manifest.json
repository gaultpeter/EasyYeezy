{
    "manifest_version": 2,
    "name": "Ez-Yeezy",
    "description": "Splash screen auto refresh",
    "version": "0.1",
    "permissions": [
        "cookies",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Ez-Yeezy"
    },
    "author": {
        "name": "Peter G",
        "email": "peteragault2@gmail.com"
    }
}